<mvc:View controllerName="lvcrft.lovecraftcollection.controller.Master"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
		<ObjectHeader
		title="{i18n>Master_Title}"
		class="sapUiResponsivePadding--header" >
		<ObjectAttribute text="{i18n>Prop 1}" />
		<ObjectAttribute text="{i18n>Prop 2}" />
		<ObjectAttribute text="{i18n>Prop 3}" />
		<ObjectAttribute
			text="{i18n>wikipedia}"
			active="true"
			press="handleLinkObjectAttributePress" />
	</ObjectHeader>


	<List
		headerText="{i18n>Authors_List_Title}"
		items="{
			path: 'Authors>/authors'
		}" >
		<StandardListItem
			title="{Authors>/Name} {Authors>/Lastname}"
			description="{Authors>/Nationality}"
        />
	</List>
        </content> 
    </Page>
</mvc:View>
